PORT=4000
OPENAI_API_KEY=your_openai_key_here
DATABASE_URL=postgresql://user:pass@localhost:5432/asknewton
REDIS_URL=redis://localhost:6379
NODE_ENV=development

# Authentication (comma-separated API keys)
# Leave empty in development to disable auth
API_KEYS=key1,key2,key3

# Rate Limiting (requests per minute per IP)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=60

# Covered California API (for real plan data)
COVERED_CA_API_KEY=your_covered_ca_key_here
